    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-pwa": "^0.17.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
